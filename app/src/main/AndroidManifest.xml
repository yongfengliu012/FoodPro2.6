        <activity android:name="xwang.george.foodpro2.AtyMealAnalyzing"></activity>
        <activity android:name="xwang.george.foodpro2.AtyMainMenu"></activity>
        <activity android:name="xwang.george.foodpro2.AtyFindPassword"></activity>
        <provider
